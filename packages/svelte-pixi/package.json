{
  "name": "svelte-pixi",
  "version": "0.0.1-alpha.1",
  "description": "Create Pixi.js apps with Svelte 3",
  "repository": "https://github.com/mattjennings/svelte-pixi",
  "author": "Matt Jennings",
  "license": "MIT",
  "svelte": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "build": "tsc --outDir ./dist --declaration && node scripts/preprocess.js",
    "dev": "nodemon --watch src --ext js,ts,svelte,json --exec \"npm run build\"",
    "validate": "svelte-check",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "pixi.js": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "nodemon": "^2.0.6",
    "rollup": "^2.32.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.24.0",
    "svelte-check": "^1.0.60",
    "svelte-preprocess": "^4.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "pixi.js": "^5.3.3"
  },
  "keywords": [
    "svelte"
  ]
}
