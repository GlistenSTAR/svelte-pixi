{
  "name": "svelte-pixi",
  "version": "0.1.0-alpha.1",
  "description": "Create Pixi.js apps with Svelte 3",
  "repository": "https://github.com/mattjennings/svelte-pixi",
  "author": "Matt Jennings",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "svelte-check --threshold error --ignore \"dist\"",
    "prepublishOnly": "npm run build",
    "publish:prod": "npm publish ./package --tag latest",
    "publish:next": "npm publish ./package --tag next"
  },
  "devDependencies": {
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "mdsvex": "^0.10.5",
    "mdsvex-sveld": "^1.0.3",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.27.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.5",
    "tailwindcss": "^3.0.12",
    "tslib": "^2.3.1",
    "vite-plugin-sveld": "^1.0.2",
    "eslint": "^8.10.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "pixi.js": "^6.0.0",
    "prettier": "^2.6.0",
    "typescript": "~4.5.5"
  },
  "peerDependencies": {
    "pixi.js": "^6.0.0"
  },
  "type": "module"
}
