{
  "name": "svelte-pixi",
  "version": "0.1.0",
  "description": "Create PixiJS apps with Svelte",
  "repository": "https://github.com/mattjennings/svelte-pixi",
  "author": "Matt Jennings",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build:site": "svelte-kit build",
    "build": "svelte-kit package",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "validate": "svelte-check --tsconfig ./tsconfig.json --threshold error --ignore \"dist\"",
    "prepare": "svelte-kit sync && husky install",
    "release": "release-it --npm.tag=latest",
    "prerelease": "release-it --preRelease=beta --npm.tag=next",
    "lint": "eslint .",
    "prettier": "prettier --ignore-path .eslintignore --plugin-search-dir=. . --check",
    "format": "prettier --ignore-path .eslintignore --plugin-search-dir=. . --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@release-it/conventional-changelog": "^5.0.0",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-vercel": "^1.0.0-next.50",
    "@sveltejs/kit": "^1.0.0-next.324",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "dashify": "^2.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "fathom-client": "^3.4.1",
    "husky": "^7.0.0",
    "just-camel-case": "^6.0.1",
    "mdsvex": "^0.10.5",
    "mdsvex-sveld": "^1.0.3",
    "mdsvexamples": "^0.3.0-beta.1",
    "mousetrap": "^1.6.5",
    "pixi.js": "^6.0.0",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
    "prettier": "^2.6.0",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.27.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "release-it": "^15.0.0",
    "sveld": "^0.15.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-focus-trap": "^1.2.0",
    "svelte-intersection-observer": "^0.10.0",
    "svelte-preprocess": "^4.10.4",
    "svelte2tsx": "^0.5.6",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5",
    "unist-util-visit": "^4.1.0",
    "vite": "^2.8.6",
    "vite-plugin-sveld": "^1.0.3"
  },
  "peerDependencies": {
    "pixi.js": "^6.0.0"
  },
  "type": "module"
}
